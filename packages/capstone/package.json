{
  "name": "capstone-wasm",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/capstone-wasm.umd.cjs",
  "module": "dist/capstone-wasm.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/CzBiX/disasm-web.git",
    "directory": "packages/capstone"
