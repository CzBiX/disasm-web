    "dev": "tsc -w",
    "test": "vitest"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.34.0"
  },
  "files": [
    "dist"
  ]
}
